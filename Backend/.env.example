# Supabase Configuration (server - required for Backend)
# Use your Supabase project URL and the SERVICE ROLE KEY (server-only, keep secret)
VITE_SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Server Configuration
PORT=3000
NODE_ENV=development

# CORS Configuration
# For development: http://localhost:5173,http://localhost:5174
# For production: https://your-frontend-domain.netlify.app,https://your-frontend-domain.netlify.app/admin
FRONTEND_URL=http://localhost:5173,http://localhost:5174

# Security
ADMIN_SECRET=your-secret-admin-key
HELMET_ENABLED=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=15000
RATE_LIMIT_MAX_REQUESTS=100


# Security
HELMET_ENABLED=true